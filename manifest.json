{
  "name": "Sound Tab",
  "version": "0.1",
  "description": "Quickly switch to tabs that are making sounds",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Switch to audible tab"
  },
  "permissions": [
    "tabs",
    "windows"
  ],
  "commands": {
    "sound_tab_command": {
      "suggested_key": {
        "default": "Alt+Shift+S"
      },
      "description": "Switch to audible tab"
    }
  },
  "minimum_chrome_version": "88",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
