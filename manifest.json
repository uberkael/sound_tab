  {
    "name": "Sound Tab",
    "version": "0.1",
    "description": "Quickly switch to tabs that are making sounds",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Switch to audible tab"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Alt+Shift+S"
        },
        "description": "Switch to audible tab"
      }
    },
    "manifest_version": 2,
    "minimum_chrome_version": "45",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }

  }